HIDEM
c$=MKI$(&H200)+MKI$(&H201)+MKI$(&H202)+MKI$(&H203)+MKI$(&H204)+MKI$(&H205)+MKI$(&H206)+MKI$(&H207)+MKI$(&H211)+MKI$(&H222)+MKI$(&H333)+MKI$(&H444)+MKI$(&H555)+MKI$(&H666)+MKI$(&H777)+MKI$(&H200)+MKI$(&H200)
a$=STRING$(1000,"020306040705080910111412151301")
DIM x(10000),y(10000)
WHILE k=0
  h=h+1+(11 AND h=0)
  MOUSE x(h),y(h),k
