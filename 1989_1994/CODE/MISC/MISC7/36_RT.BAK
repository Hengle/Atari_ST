************ ROUTINE DE CALCUL DES X *************

CALCUL	MOVE.L	A4,-(SP)
	LEA	ADR_RTS,A4
	MOVEQ	#0,D4
	CMP	D0,D2
	BGT.S	D2_P_G
	EXG.L	D0,D2
	BRA	OTHER
D2_P_G	SUB.W	D0,D2	DELTA X
	SUB.W	D1,D3	DELTA Y
	BNE.S	NON_NUL
	MOVE.L	(SP)+,A4
	RTS
NON_NUL	MOVE.W	D2,D4
	ASL	#7,D4	*128
	DIVS.W	D3,D4	*X(+1)=(DX/DY)+X
	MOVE.W	#9,D2
	EXT.L	D4
	ASL.L	D2,D4
	ADD	D3,D3
	ADD	D3,D3
	NEG	D3
	SWAP	D4
	SUB	D1,D1
	JMP	(A4,D3.W)
	REPT	201
	MOVE.W	D0,(A3)+
	ADDX.L	D4,D0
	ENDR
ADR_RTS	MOVE.L	(SP)+,A4
	RTS

OTHER	LEA	ADR_RTS2,A4
	SUB.W	D0,D2	DELTA X
	SUB.W	D1,D3	DELTA Y
	BNE.S	NON_NUL2
	MOVE.L	(SP)+,A4
	RTS
