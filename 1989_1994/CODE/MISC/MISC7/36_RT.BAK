	MOVE.L	D3,D5	;D5=Y2
	MULS.W	D0,D5	;D5=X1*Y2
	SUB.L	D4,D5	;D5=X1Y2-X2Y1
	MOVE.L	D5,D4
	MOVE.L	D0,D5
