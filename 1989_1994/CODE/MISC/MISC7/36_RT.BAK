	ADD	D5,D5
	ADD	D5,A3
	MOVE.W	D2,D4
	ASL	#7,D4	*128
	DIVS.W	D3,D4	*X(+1)=(DX/DY)+X
	MOVE.W	#9,D2
	EXT.L	D4
	ASL.L	D2,D4
	ADD	D3,D3
	ADD	D3,D3
	NEG	D3
	SWAP	D4
	SUB	D1,D1
	JMP	(A4,D3.W)
	REPT	201
	MOVE.W	D0,-(A3)
	ADDX.L	D4,D0
	ENDR
ADR_RTS2	MOVE.L	(SP)+,A4
	RTS

